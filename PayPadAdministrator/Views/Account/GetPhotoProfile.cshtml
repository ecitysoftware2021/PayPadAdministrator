@model PayPadAdministrator.Models.UserViewModel

@{
    var imgSrc = "~/Content/Images/User_Generic_1.png";
    if (Model != null && Model.IMAGE != null)
    {
        var base64 = Convert.ToBase64String(Model.IMAGE);
        imgSrc = String.Format("data:image/gif;base64,{0}", base64);
    }

}

<img src="@imgSrc" alt="user-img" class="img-circle"> 