@model IEnumerable<PayPlusModels.SP_GET_UPLOAD_DETAILS_Result>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>GetUploads</title>
</head>
<body>
    <div id="portlet2" class="panel-collapse collapse in">
        <div class="portlet-body">
            <div class="table-responsive">
                <table class="table table-striped table-bordered display nowrap"
                       id="tableTransactions">
                    <thead>
                        <tr>
                            <th>
                                VALOR CARGADO
                            </th>
                            <th>
                                VALOR PROGRAMADO
                            </th>

                            <th>
                                DENOMINACIÓN
                            </th>
                            <th>
                                DESCRIPCIÓN
                            </th>
                            <th></th>
                        </tr>


                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => item.AMOUNT)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.AMOUNT_NEW)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.VALUE)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.DESCRIPTION)
                                </td>
                            </tr>
                        }
                </thead>
                </table>
            </div>
        </div>
    </div>
</body>
</html>
