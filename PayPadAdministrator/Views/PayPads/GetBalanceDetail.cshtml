@model IEnumerable<PayPlusModels.BalancingDetailResultViewModel>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>GetBalanceDetail</title>
</head>
<body>
    <div id="portlet2" class="panel-collapse collapse in">
        <div class="portlet-body">
            <div class="table-responsive">
                <table class="table table-striped table-bordered display nowrap"
                       id="tableTransactions">
                    <thead>
                        <tr>
                            <th>
                                VALOR PROGRAMADO
                            </th>
                            <th>
                                VALOR RECOLECTADO
                            </th>
                            <th>
                                DENOMINACIÓN
                            </th>
                            <th>
                                TIPO DISPOSITIVO
                            </th>
                            <th>
                                DESCRIPCIÓN
                            </th>
                        </tr>

                        @foreach (var item in Model)
                        {
                            <tr>

                                <td>
                                    @Html.DisplayFor(modelItem => item.AMOUNT)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.AMOUNT_NEW)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.VALUE)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.DESCRIPTION)
                                </td>

                            </tr>
                        }

                    </thead>
                </table>
            </div>
        </div>
    </div>
</body>
</html>
