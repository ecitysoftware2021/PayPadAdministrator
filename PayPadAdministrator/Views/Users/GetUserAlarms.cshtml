@model IEnumerable<PayPlusModels.UserAlarm>
@{
    ViewBag.Title = "Usuarios de alarma";
}

<div class="row">
    <div class="col-sm-8">
        <a href="@Url.Action("CreateUserAlarms","Users")" class="btn btn-default btn-md waves-effect waves-light m-b-30"
           data-overlaySpeed="200" data-overlayColor="#36404a"><i class="md md-add"></i>Crear Usuario Alarma</a>
    </div>
    <div class="col-sm-4">

    </div>
</div>

<div class="row">
    @foreach (var item in Model)
    {
        <div class="col-sm-6 col-lg-4">
            <div class="card-box">
                <div class="contact-card">
                    <a class="pull-left" href="#">
                    </a>
                    <div class="member-info">
                        <h4 class="m-t-0 m-b-5 header-title"><b>@Html.DisplayFor(modelItem => item.USER_NAME)</b></h4>
                        <p class="text-muted">@Html.DisplayFor(modelItem => item.ALARM_NAME)</p>
                        <p class="text-dark"><i class="md md-account-circle m-r-10"></i><small>@Html.DisplayFor(modelItem => item.NAME)</small></p>
                        <p class="text-dark"><i class="md md-business m-r-10"></i><small>@Html.DisplayFor(modelItem => item.DESACTIVATE_CODE)</small></p>                        
                        <div class="contact-action">
                            <a href="#" class="btn btn-success btn-sm"><i class="md md-mode-edit"></i></a>
                            <a href="#" class="btn btn-danger btn-sm"><i class="md md-close"></i></a>
                        </div>
                    </div>

                    <ul class="social-links list-inline m-0">

                    </ul>
                </div>
            </div>

        </div> <!-- end col -->
    }


</div>